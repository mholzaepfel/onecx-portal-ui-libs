{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "baseUrl": "./",
    "paths": {
      "tenant/*": ["tenant/*"],
      "theme/*": ["theme/*"],
      "assignments/*": ["assignments/*"],
      "workspace/*": ["workspace/*"],
      "product-store/*": ["product-store/*"],
      "permissions/*": ["permissions/*"]
    }
  }
}
